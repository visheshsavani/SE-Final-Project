{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "Food_Delivery_Web/frontend/frontend/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "Food_Delivery_Web/admin/index.html"
    }
  ]
}